        // Show the iframe div with rotation and fade-in animation
        function showIframeDiv() {
            const iframeDiv = document.getElementById('iframeDiv');
            const giftDiv = document.getElementById('giftDiv');

            // Hide the gift div
            giftDiv.style.display = 'none';

            // Show and animate the iframe div
            iframeDiv.style.display = 'block'; 
        }

        // Cancel the free gift popup
        function cancelGift(event) {
            event.stopPropagation(); // Prevent triggering the iframe display
